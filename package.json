{
  "name": "dutchess-coinbase-trading-bot",
  "version": "1.0.0",
  "description": "Real-time Bitcoin trading bot using Coinbase API",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "demo": "node cli.js start --demo",
    "test": "jest",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "cleanup": "node scripts/cleanup.js",
    "cleanup:3000": "node scripts/cleanup.js 3000",
    "build:frontend": "cd frontend && npm run build",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:frontend",
    "start:full": "npm run build && npm start",
    "start:with-frontend": "node start-with-frontend.js",
    "test:frontend": "node test-frontend.js"
  },
  "keywords": [
    "coinbase",
    "bitcoin",
    "trading",
    "bot",
    "cryptocurrency"
  ],
  "author": "DutchessAI",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.7",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "socket.io": "^4.8.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
